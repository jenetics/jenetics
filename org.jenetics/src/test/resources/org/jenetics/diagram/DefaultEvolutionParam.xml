<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<evolution-param>
	<survivor-selector xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="org.jenetics.TournamentSelector" samples="5"/>
	<offspring-selector xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="org.jenetics.RouletteWheelSelector"/>
	<alterer xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="org.jenetics.CompositeAlterer" length="2">
		<alterers xsi:type="org.jenetics.Mutator" probability="0.03"/>
		<alterers xsi:type="org.jenetics.SinglePointCrossover" probability="0.125"/>
	</alterer>
	<population-size>150</population-size>
	<offspring-fraction>0.6</offspring-fraction>
	<maximal-phenotype-age>70</maximal-phenotype-age>
</evolution-param>
