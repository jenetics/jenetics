key "Fortin2013"
literal "true"

\end_inset

, 
\begin_inset CommandInset citation
LatexCommand cite
key "Deb:2002:FEM:2221359.2221582"
literal "true"

\end_inset

 and 
\begin_inset CommandInset citation
LatexCommand cite
key "Konak2006"
literal "true"

\end_inset

.
