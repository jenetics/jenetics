<suite name="Jenetix" verbose="2" parallel="tests" thread-count="5" >
	<test name="org.jenetix.LargeIntegerChromosome">
		<classes>
			<class name="org.jenetix.LargeIntegerChromosomeTest" />
		</classes>
	</test>
	<test name="org.jenetix.LargeIntegerGene">
		<classes>
			<class name="org.jenetix.LargeIntegerGeneTest" />
		</classes>
	</test>
	<test name="org.jenetix.util.NumberRandom">
		<classes>
			<class name="org.jenetix.util.LargeIntegerRandomTest" />
			<class name="org.jenetix.util.LCG32ShiftRandomTest" />
		</classes>
	</test>
</suite> 
